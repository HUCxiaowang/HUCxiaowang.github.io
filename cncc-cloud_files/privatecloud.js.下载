
//他们选择并相信我们
function ancolvie(n) {
	for (var i = 1; i <= 8; i++) {
		if (i == n) {
			$('#tab2_0' + i).removeClass("undis");
			$('#tab2_0' + i).addClass("dis");
			$('#tab2_' + i).removeClass("nonav");
			$('#tab2_' + i).addClass("nav_on6");
		} else {
			$('#tab2_' + i).removeClass("nav_on6");
			$('#tab2_' + i).addClass("nonav");
			$('#tab2_0' + i).removeClass("dis");
			$('#tab2_0' + i).addClass("undis");
		}
	}
}
//私有云平台基础服务
function service(n) {
	for (var i = 1; i <= 8; i++) {
		if (i == n) {
			$('#tab3_0' + i).removeClass("undis");
			$('#tab3_0' + i).addClass("dis");
			$('#tab3_' + i).removeClass("nonav");
			$('#tab3_' + i).addClass("nav_on6");
		} else {
			$('#tab3_' + i).removeClass("nav_on6");
			$('#tab3_' + i).addClass("nonav");
			$('#tab3_0' + i).removeClass("dis");
			$('#tab3_0' + i).addClass("undis");
		}
	}
} 

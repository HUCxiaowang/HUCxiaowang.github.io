<script type="text/javascript" charset="utf-8">
	var login = function(){ 
		var service = 'https://www.kuaiyun.cn';
	// 	if(service&&service.indexOf("https")>=0){
	// 		service = "http"+service.substring(5);
	// 	}
        var ff = false;
        $.ajax({
            async:false,
            type : "POST",
            url : "checkRand",
            data : {"rand": $("input[name^='rand']").val()},
            dataType: "json",
            success : function(data) {
                if(data && data.status == 'y'){
                    ff = true;
                    $("#msg").hide();
                }else{
                    $("#errordiv").show();
                    $("#error").html(data.info);
                    $("#randImg").click();
                }
            },
            error : function(e) {
                console.info("验证码验证失败。");
            }

        });
        if(ff){
			$.ajax({ url: 'https://ac.kuaiyun.cn/cas/login?mode=torlogin&service='+service+'/kyunmember/ssoresult.action',
				dataType: "jsonp", jsonpCallback: "jsonpcallback",
				success: function (data) {
					$('#lt').val(data.lt);
					$('#execution').val(data.execution);
					$('#uuid').val(data.uuid);
					$('#password').val(encrypt($("#password1").val()));
					$('#login-form').submit();
					},
					error:function(){
						alert('网络访问错误!');
						}
					});
		}
	};
var logincallback = function(result) {
		if (result.ret == 0){
			var str=$("#login-form").attr("action")
			location.href=str.substr(str.lastIndexOf('service=')+8);
		} else if(result.ret==-7){//账号被锁定
			$("#errordiv").show();
			$("#error").html("帐户被锁定，"+result.msg+"分钟后自动解锁或<a href\='https://www.kuaiyun.cn:443/kyunmember/backpasswordno1' target\='_blank' class='logint-link'>点击这里</a>解锁");
			$("#username").val(result.username);
			$("#password1").val('');
            $("#randImg").click();
		}else{
			$("#errordiv").show();
			$("#error").html(result.msg);
			$("#username").val(result.username);
			$("#password1").val('');
            $("#randImg").click();
		}
	};
	function encrypt(word){
		if(!word){
			return;
		}
	    var key = CryptoJS.enc.Utf8.parse("S1sdfWFG401S45Y1");
	    var srcs = CryptoJS.enc.Utf8.parse(word);
	    var encrypted = CryptoJS.AES.encrypt(srcs, key, {mode:CryptoJS.mode.ECB,padding: CryptoJS.pad.Pkcs7});
	    return encrypted.toString();
	}
</script>
